{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": "public",
  "env": {
    "BACKEND_API": "https://1332551170-4tuhxe4fzv.ap-guangzhou.tencentscf.com"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/((?!api/).*)$",
      "destination": "/index.html"
    }
  ]
}
